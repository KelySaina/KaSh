{
  "name": "kash-money-management",
  "version": "1.0.0",
  "description": "Full-scale money management app with Konnect Service integration",
  "main": "server/server.js",
  "scripts": {
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "migrate": "node server/database/migrate.js",
    "seed": "node server/database/seed.js",
    "build": "cd client && npm run build"
  },
  "keywords": ["money", "finance", "budget", "expense-tracker"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
